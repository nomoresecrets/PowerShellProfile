{
  "name": "Chester",
  "description": "Console scheme based on the Chester Atom syntax theme.",
  "repository": "https://github.com/mmims",
  "background": "Black",
  "foreground": "DarkCyan",
  "paletteFormat": "BGR",
  "palette": {
    "Black":       "0x43362c",
    "DarkBlue":    "0xe5be8a",
    "DarkGreen":   "0xaae6c7",
    "DarkCyan":    "0xb3a999",
    "DarkRed":     "0x3fc8ff",
    "DarkMagenta": "0xc4716c",
    "DarkYellow":  "0x7c7467",
    "Gray":        "0xece6db",
    "DarkGray":    "0x4f443b",
    "Blue":        "0xd68a28",
    "Green":       "0x8dc916",
    "Cyan":        "0xdede27",
    "Red":         "0x5b5efa",
    "Magenta":     "0x8e70ff",
    "Yellow":      "0x6deffe",
    "White":       "0xffffff"
  },
  "tokens": {
    "readline": {
      "foreground": {
        "Command":      "Blue",
        "Comment":      "DarkGreen",
        "ContinuationPrompt": "Gray",
        "DefaultToken": "Gray",
        "Emphasis":     "Cyan",
        "Error":        "Red",
        "Keyword":      "Green",
        "Member":       "Blue",
        "Number":       "Magenta",
        "Operator":     "Green",
        "Parameter":    "Gray",
        "String":       "Cyan",
        "Type":         "DarkYellow",
        "Variable":     "Blue"
      }
    },
    "write": {
      "foreground": {
        "Error":    "Red",
        "Warning":  "Yellow",
        "Verbose":  "Yellow",
        "Debug":    "Yellow",
        "Progress": "Yellow"
      },
      "background": {
        "Progress": "DarkCyan"
      }
    }
  }
}