{
  "name": "Bespin",
  "description": "Bespin scheme by Jan T. Sott.",
  "repository": "https://github.com/mmims",
  "background": "Black",
  "foreground": "Green",
  "popupBackground": "Cyan",
  "popupForeground": "Gray",
  "palette": {
    "Black": "#28211c",
    "DarkGray": "#36312e",
    "Cyan": "#5e5d5c",
    "Blue": "#666666",
    "Yellow": "#797977",
    "Green": "#8a8986",
    "Gray": "#9d9b97",
    "White": "#baae9e",
    "DarkRed": "#cf6a4c",
    "Red": "#cf7d34",
    "DarkYellow": "#f9ee98",
    "DarkGreen": "#54be0d",
    "DarkCyan": "#afc4db",
    "DarkBlue": "#5ea6ea",
    "DarkMagenta": "#9b859d",
    "Magenta": "#937121"
  },
  "tokens": {
    "readline": {
      "foreground": {
        "Command": "DarkBlue",
        "Comment": "Yellow",
        "ContinuationPrompt": "Green",
        "DefaultToken": "Green",
        "Emphasis": "DarkCyan",
        "Error": "DarkRed",
        "Keyword": "DarkMagenta",
        "Member": "White",
