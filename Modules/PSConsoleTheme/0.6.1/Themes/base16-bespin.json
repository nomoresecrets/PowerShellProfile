        "Operator": "Green",
        "Parameter": "Blue",
        "String": "DarkGreen",
        "Type": "DarkMagenta",
        "Variable": "Red"
      }
    },
    "write": {
      "foreground": {
        "Error": "DarkRed",
        "Warning": "DarkYellow",
        "Verbose": "DarkYellow",
        "Debug": "DarkYellow",
        "Progress": "Yellow"
      },
      "background": {
        "Progress": "DarkGray"
