{
  "name": "Hopscotch",
  "description": "Hopscotch scheme by Jan T. Sott.",
  "repository": "https://github.com/mmims",
  "background": "Black",
  "foreground": "Green",
  "popupBackground": "Cyan",
  "popupForeground": "Gray",
  "palette": {
    "Black": "#322931",
    "DarkGray": "#433b42",
    "Cyan": "#5c545b",
    "Blue": "#797379",
    "Yellow": "#989498",
    "Green": "#b9b5b8",
    "Gray": "#d5d3d5",
    "White": "#ffffff",
    "DarkRed": "#dd464c",
    "Red": "#fd8b19",
    "DarkYellow": "#fdcc59",
    "DarkGreen": "#8fc13e",
    "DarkCyan": "#149b93",
    "DarkBlue": "#1290bf",
    "DarkMagenta": "#c85e7c",
    "Magenta": "#b33508"
  },
  "tokens": {
    "readline": {
      "foreground": {
        "Command": "DarkBlue",
        "Comment": "Yellow",
        "ContinuationPrompt": "Green",
        "DefaultToken": "Green",
        "Emphasis": "DarkCyan",
        "Error": "DarkRed",
        "Keyword": "DarkMagenta",
        "Member": "White",
        "Number": "White",
        "Operator": "Green",
        "Parameter": "Blue",
        "String": "DarkGreen",
        "Type": "DarkMagenta",
        "Variable": "Red"
      }
    },
    "write": {
      "foreground": {
        "Error": "DarkRed",
        "Warning": "DarkYellow",
        "Verbose": "DarkYellow",
        "Debug": "DarkYellow",
        "Progress": "Yellow"
      },
      "background": {
        "Progress": "DarkGray"
      }
    }
  }
}