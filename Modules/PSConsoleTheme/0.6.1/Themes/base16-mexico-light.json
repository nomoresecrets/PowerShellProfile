{
  "name": "Mexico Light",
  "description": "Mexico Light scheme by Sheldon Johnson.",
  "repository": "https://github.com/mmims",
  "background": "Black",
  "foreground": "Green",
  "popupBackground": "Cyan",
  "popupForeground": "Gray",
  "palette": {
    "Black": "#f8f8f8",
    "DarkGray": "#e8e8e8",
    "Cyan": "#d8d8d8",
    "Blue": "#b8b8b8",
    "Yellow": "#585858",
    "Green": "#383838",
    "Gray": "#282828",
    "White": "#181818",
    "DarkRed": "#ab4642",
    "Red": "#dc9656",
    "DarkYellow": "#f79a0e",
    "DarkGreen": "#538947",
    "DarkCyan": "#4b8093",
    "DarkBlue": "#7cafc2",
