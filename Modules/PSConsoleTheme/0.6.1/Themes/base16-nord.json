{
  "name": "Nord",
  "description": "Nord scheme by arcticicestudio.",
  "repository": "https://github.com/mmims",
  "background": "Black",
  "foreground": "Green",
  "popupBackground": "Cyan",
  "popupForeground": "Gray",
  "palette": {
    "Black": "#2e3440",
    "DarkGray": "#3b4252",
    "Cyan": "#434c5e",
    "Blue": "#4c566a",
