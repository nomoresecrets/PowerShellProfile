{
  "name": "Flat",
  "description": "Flat scheme by Chris Kempson (http://chriskempson.com).",
  "repository": "https://github.com/mmims",
  "background": "Black",
  "foreground": "Green",
  "popupBackground": "Cyan",
  "popupForeground": "Gray",
  "palette": {
    "Black": "#2c3e50",
    "DarkGray": "#34495e",
    "Cyan": "#7f8c8d",
    "Blue": "#95a5a6",
